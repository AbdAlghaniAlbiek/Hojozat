    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <ResourceDictionary.MergedDictionaries>
                <ColorPaletteResources Accent="#FF9676B8" AltHigh="#FF000000" AltLow="#FF000000" AltMedium="#FF000000" AltMediumHigh="#FF000000" AltMediumLow="#FF000000" BaseHigh="#FFFFFFFF" BaseLow="#FF9676B8" BaseMedium="#FFCDB9E3" BaseMediumHigh="#FFDBCAEE" BaseMediumLow="#FFB298CE" ChromeAltLow="#FFDBCAEE" ChromeBlackHigh="#FF000000" ChromeBlackLow="#FFDBCAEE" ChromeBlackMedium="#FF000000" ChromeBlackMediumLow="#FF000000" ChromeDisabledHigh="#FF9676B8" ChromeDisabledLow="#FFCDB9E3" ChromeGray="#FFBFA8D8" ChromeHigh="#FFBFA8D8" ChromeLow="#FF502F77" ChromeMedium="#FF614187" ChromeMediumLow="#FF8464A8" ChromeWhite="#FFFFFFFF" ListLow="#FF614187" ListMedium="#FF9676B8" />
                <ResourceDictionary>
                    <Color x:Key="SystemChromeAltMediumHighColor">#CC000000</Color>
                    <Color x:Key="SystemChromeAltHighColor">#FF000000</Color>
                    <Color x:Key="SystemRevealListLowColor">#FF614187</Color>
                    <Color x:Key="SystemRevealListMediumColor">#FF9676B8</Color>
                    <Color x:Key="RegionColor">#FF000000</Color>
                    <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <ResourceDictionary.MergedDictionaries>
                <ColorPaletteResources Accent="#FF9676B8" AltHigh="#FFFFFFFF" AltLow="#FFFFFFFF" AltMedium="#FFFFFFFF" AltMediumHigh="#FFFFFFFF" AltMediumLow="#FFFFFFFF" BaseHigh="#FF000000" BaseLow="#FF9676B8" BaseMedium="#FF614187" BaseMediumHigh="#FF3E1D67" BaseMediumLow="#FF502F77" ChromeAltLow="#FF3E1D67" ChromeBlackHigh="#FF000000" ChromeBlackLow="#FF9676B8" ChromeBlackMedium="#FF3E1D67" ChromeBlackMediumLow="#FF614187" ChromeDisabledHigh="#FF9676B8" ChromeDisabledLow="#FF614187" ChromeGray="#FF502F77" ChromeHigh="#FF9676B8" ChromeLow="#FFDBCAEE" ChromeMedium="#FFCDB9E3" ChromeMediumLow="#FFDBCAEE" ChromeWhite="#FFFFFFFF" ListLow="#FFCDB9E3" ListMedium="#FF9676B8" />
                <ResourceDictionary>
                    <Color x:Key="SystemChromeAltMediumHighColor">#CCFFFFFF</Color>
                    <Color x:Key="SystemChromeAltHighColor">#FFFFFFFF</Color>
                    <Color x:Key="SystemRevealListLowColor">#FFCDB9E3</Color>
                    <Color x:Key="SystemRevealListMediumColor">#FF9676B8</Color>
                    <RevealBackgroundBrush x:Key="SystemControlHighlightListLowRevealBackgroundBrush" TargetTheme="Light" Color="{ThemeResource SystemRevealListMediumColor}" FallbackColor="{ StaticResource SystemListMediumColor}" />
                    <Color x:Key="RegionColor">#FFFFFFFF</Color>
                    <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <StaticResource x:Key="RegionColor" ResourceKey="SystemColorWindowColor" />
            <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>




</ResourceDictionary>
